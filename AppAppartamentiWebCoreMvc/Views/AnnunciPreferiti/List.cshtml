@model IEnumerable<AnnunciDtoOutput>

@{
    ViewData["Title"] = "View";
}

<div class="container container-custom">
    <div class="col-sm-8 offset-sm-2">
        <label class="title">
            Preferiti
        </label>

        <div id="annunci">
            <div class="text-center">
                <div id="chat-spinner-loading" class="mt-5" role="status">
                    <span class="sr-only">Loading...</span>
                    <lottie-player src="https://assets9.lottiefiles.com/datafiles/de5fLwslkvYN9zP/data.json" background="transparent" speed="1" style="width: 200px; height: 200px;" loop autoplay>
                    </lottie-player>
                </div>
              
            </div>
        </div>
    </div>
</div>

@section Scripts {
    <script type="text/javascript">
        var UrlRefresh = "@Url.Action("RefreshList", "AnnunciPreferiti" )";
    </script>

    <script src="~/js/annunci-list.js" asp-append-version="true"></script>
}